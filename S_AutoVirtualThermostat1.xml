<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upn-org:service-1-0">
    <specVersion>
        <major>1</major>
        <minor>0</minor>
    </specVersion>
    <serviceStateTable>
        <stateVariable sendEvents="yes">
            <name>Failure</name>
            <dataType>ui4</dataType>
            <defaultValue>0</defaultValue>
            <shortCode>failure</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>CycleTime</name>
            <dataType>string</dataType>
            <defaultValue></defaultValue>
            <shortCode>cycletime</shortCode>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>SetpointCooling</name>
            <dataType>i4</dataType>
            <defaultValue>72</defaultValue>
            <shortCode>coolsp</shortCode>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>SetpointHeating</name>
            <dataType>i4</dataType>
            <defaultValue>72</defaultValue>
            <shortCode>heatsp</shortCode>
        </stateVariable>
    </serviceStateTable>
    <actionList>
        <action>
            <name>SetCurrentSetpoint</name>
            <argumentList>
                <argument>
                    <name>Application</name>
                    <direction>in</direction>
                </argument>
                <argument>
                    <name>NewCurrentSetpoint</name>
                    <direction>in</direction>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>GetCurrentSetpoint</name>
            <argumentList>
                <argument>
                    <name>Application</name>
                    <direction>in</direction>
                </argument>
                <argument>
                   <name>CurrentSetpoint</name>
                   <direction>out</direction>
                   <relatedStateVariable>CurrentSetpoint</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetCoolingSetpoint</name>
            <argumentList>
                <argument>
                    <name>NewCoolingSetpoint</name>
                    <direction>in</direction>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetHeatingSetpoint</name>
            <argumentList>
                <argument>
                    <name>NewHeatingSetpoint</name>
                    <direction>in</direction>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetDebug</name>
            <argumentList>
                <argument>
                    <name>debug</name>
                    <direction>in</direction>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>getinfo</name>
            <argumentList>
                <argument>
                    <name>eventList</name>
                    <direction>out</direction>
                    <relatedStateVariable>int_el</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
    </actionList>
</scpd>
